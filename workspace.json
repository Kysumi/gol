{
  "version": 1,
  "projects": {
    "console": {
      "root": "apps/console",
      "sourceRoot": "apps/console/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/console",
            "index": "apps/console/src/index.html",
            "main": "apps/console/src/main.tsx",
            "polyfills": "apps/console/src/polyfills.ts",
            "tsConfig": "apps/console/tsconfig.app.json",
            "assets": [
              "apps/console/src/favicon.ico",
              "apps/console/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/console/src/environments/environment.ts",
                  "with": "apps/console/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "console:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "console:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/console/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/console/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "console-e2e": {
      "root": "apps/console-e2e",
      "sourceRoot": "apps/console-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/console-e2e/cypress.json",
            "tsConfig": "apps/console-e2e/tsconfig.e2e.json",
            "devServerTarget": "console:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "console:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/console-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "bestagon": {
      "root": "apps/bestagon",
      "sourceRoot": "apps/bestagon/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "all": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "nx serve electron"
              },
              {
                "command": "nx serve console --port=8080"
              },
              {
                "command": "nx serve bestagon"
              }
            ],
            "parallel": true
          }
        },
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/bestagon",
            "index": "apps/bestagon/src/index.html",
            "main": "apps/bestagon/src/main.ts",
            "polyfills": "apps/bestagon/src/polyfills.ts",
            "tsConfig": "apps/bestagon/tsconfig.app.json",
            "assets": [
              "apps/bestagon/src/favicon.ico",
              "apps/bestagon/src/assets"
            ],
            "styles": ["apps/bestagon/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/bestagon/src/environments/environment.ts",
                  "with": "apps/bestagon/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "bestagon:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "bestagon:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/bestagon/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/bestagon/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "bestagon-e2e": {
      "root": "apps/bestagon-e2e",
      "sourceRoot": "apps/bestagon-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/bestagon-e2e/cypress.json",
            "tsConfig": "apps/bestagon-e2e/tsconfig.e2e.json",
            "devServerTarget": "bestagon:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "bestagon:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/bestagon-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "electron": {
      "root": "apps/electron",
      "sourceRoot": "apps/electron/src",
      "projectType": "application",
      "prefix": "electron",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "nx-electron:build",
          "options": {
            "outputPath": "dist/apps/electron",
            "main": "apps/electron/src/main.ts",
            "tsConfig": "apps/electron/tsconfig.app.json",
            "assets": ["apps/electron/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/electron/src/environments/environment.ts",
                  "with": "apps/electron/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "nx-electron:execute",
          "options": {
            "buildTarget": "electron:build"
          }
        },
        "package": {
          "builder": "nx-electron:package",
          "options": {
            "name": "electron",
            "frontendProject": "bestagon",
            "outputPath": "dist/packages",
            "prepackageOnly": true
          }
        },
        "make": {
          "builder": "nx-electron:make",
          "options": {
            "name": "electron",
            "frontendProject": "bestagon",
            "outputPath": "dist/executables"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/electron/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/electron/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "console"
}
